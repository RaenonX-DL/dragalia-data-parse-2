{
  "name": "dragalia-data-parse-2",
  "version": "1.0.0",
  "description": "Rewritten Dragalia Lost data parser.",
  "main": "./dist/main.js",
  "scripts": {
    "dev:build": "ts-node src/main.ts build",
    "dev:export": "ts-node src/main.ts export",
